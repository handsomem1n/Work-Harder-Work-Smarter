#include <iostream>
#include <cmath>

struct Point {      // 위의 구조체'struct'는 x와 y라는 두 개의 멤버 변수를 가지고 있음
  int x;
  int y;
};
                    // 이제 이 구조체를 사용하여 점 좌표를 저장할 수 있다. 
                    
int main() {
  Point p1 = { 3, 4 };
  Point p2 = { 6, 8 };
  
  int dx = p2.x - p1.x;
  int dy = p2.y - p1.y;
  
  double distance = std::sqrt(dx * dx + dy * dy);
  
  std::cout << "The distance between the two points is: " << distance << std::endl;
  
  return 0;
}
