#include <iostream>
#include <cmath>

struct Point {
  int x;
  int y;
};

int main() {
  Point p1 = { 3, 4 };
  Point p2 = { 6, 8 };
  
  int dx = p2.x - p1.x;
  int dy = p2.y - p1.y;
  
  double distance = std::sqrt(dx * dx + dy * dy);
  
  std::cout << "The distance between the two points is: " << distance << std::endl;
  
  return 0;
}
